{
  "name": "gotlockz-bot",
  "version": "4.0.0",
  "description": "A high-performance MLB Discord bot with advanced analytics, real-time updates, and AI-powered betting analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CxxxxDxxxF/gotlockz-bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "mlb",
    "betting",
    "analytics",
    "typescript",
    "ocr",
    "ai"
  ],
  "author": "GotLockz Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CxxxxDxxxF/gotlockz-bot/issues"
  },
  "homepage": "https://github.com/CxxxxDxxxF/gotlockz-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "@discordjs/rest": "^2.5.1",
    "@google-cloud/vision": "^5.2.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "axios": "^1.10.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "openai": "^5.8.2",
    "tesseract.js": "^5.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
